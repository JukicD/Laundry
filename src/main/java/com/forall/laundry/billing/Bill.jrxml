	<subDataset name="Laundry" uuid="e869ff0b-8903-465a-8e88-617bccc56348">
		<parameter name="item_ids" class="java.util.Collection">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select c."name" AS customer_name,
c."address" AS customer_address,
i.amount AS item_amount,
p."name" AS product_name,
p.price AS product_price

FROM "public".customer c, "public".item i, "public".ordery o, "public".product p

WHERE $X{IN, i.item_id::varChar, item_ids} AND o.customer_id = c.id AND o.order_id = i.ordery_id AND i.item_product_product_id = p.product_id;]]>
		</queryString>
		<field name="customer_name" class="java.lang.String"/>
		<field name="customer_address" class="java.lang.String"/>
		<field name="item_amount" class="java.lang.Integer"/>
