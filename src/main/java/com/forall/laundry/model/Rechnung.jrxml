	<subDataset name="laundry" uuid="b6d30ca6-0352-4955-8b1a-48ea3c919605">
		<queryString language="SQL">
			<![CDATA[SELECT
     customer."id" AS customer_id,
     customer."address" AS customer_address,
     customer."name" AS customer_name,
     item."amount" AS item_amount,
     product."name" AS product_name,
     product."price" AS product_price,
     ordery."time" AS ordery_time
FROM
     "public"."product" product INNER JOIN "public"."item" item ON product."product_id" = item."item_product_product_id"
     INNER JOIN "public"."ordery" ordery ON item."ordery_id" = ordery."order_id"
     INNER JOIN "public"."customer" customer ON ordery."customer_id" = customer."id"]]>
		</queryString>
		<field name="customer_id" class="java.lang.Integer"/>
		<field name="customer_address" class="java.lang.String"/>
		<field name="customer_name" class="java.lang.String"/>
		<field name="item_amount" class="java.lang.Integer"/>
		<field name="product_name" class="java.lang.String"/>
		<field name="product_price" class="java.lang.Integer"/>
		<field name="ordery_time" class="java.sql.Timestamp"/>
		<group name="customer_name">
			<groupExpression><![CDATA[$F{customer_name}]]></groupExpression>
		</group>
	</subDataset>
